_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="53"/><column index="2" value="273"/><column index="3" value="69"/><column index="4" value="36"/><column index="5" value="300"/><column index="6" value="300"/><column index="7" value="300"/><column index="8" value="140"/><column index="9" value="72"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">SELECT 
    r.name AS restaurant_name,
    r.street AS restaurant_address,
    r.overall_rating AS restaurant_rating,
    r.reviews_count AS total_reviews,
    rv.author AS review_author,
    rv.rating AS review_rating,
    rv.review_date AS review_date,
    rv.review_text AS review_text
FROM 
    restaurants r
JOIN 
    reviews rv
ON 
    r.id_restaurant = rv.id_restaurant
WHERE 
    r.name = 'Brasserie Georges'; -- Remplace par le nom du restaurant que tu veux tester
</sql><current_tab id="0"/></tab_sql></sqlb_project>
